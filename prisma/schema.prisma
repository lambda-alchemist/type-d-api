
generator client {
	provider = "prisma-client-js"
	previewFeatures = ["deno"]
	output = "./client"
}

generator dbml {
	provider = "prisma-dbml-generator"
	output = "./dbml"
}

datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
}

model User {
	id         String @id @default(uuid())
	username   String
	email      String
	created_at DateTime @default(now())
	updated_at DateTime @updatedAt
}
